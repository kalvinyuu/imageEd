([nil nil ((#(" " 0 1 (fontified t)) . -595) (undo-tree-id754 . -1) (undo-tree-id755 . -1) (undo-tree-id756 . -1) (undo-tree-id757 . -1) (undo-tree-id758 . -1) (undo-tree-id759 . -1) 596 (595 . 596) (#("i" 0 1 (fontified t)) . -595) (undo-tree-id760 . -1) 596 (595 . 596) (t 26977 4927 461960 459000)) nil (26977 5821 14062 0) 0 (:visualizer [0 1 0 #<marker at 2303 in  *undo-tree*>])])
([nil current ((4039 . 4047) (#(">" 0 1 (fontified nil)) . 4039) (4029 . 4040) (#("up" 0 1 (fontified nil) 1 2 (fontified nil)) . 4029) (4027 . 4031) (#("o" 0 1 (fontified nil)) . 4027) (4015 . 4028) (#("ASE" 0 1 (fontified nil) 1 2 (fontified nil) 2 3 (fontified nil)) . 4015) (4010 . 4018) ("Professional Decentralized Editor" . 4010) (undo-tree-id1825 . -33) (undo-tree-id1826 . -33) (undo-tree-id1827 . -33) (3890 . 3894) ("ImageEd" . 3890) (undo-tree-id1828 . -7) (#(" useEffect" 0 1 (fontified t) 1 2 (face font-lock-variable-name-face fontified t) 2 3 (face font-lock-variable-name-face fontified t) 3 4 (face font-lock-variable-name-face fontified t) 4 5 (face font-lock-variable-name-face fontified t) 5 6 (face font-lock-variable-name-face fontified t) 6 7 (face font-lock-variable-name-face fontified t) 7 8 (face font-lock-variable-name-face fontified t) 8 9 (face font-lock-variable-name-face fontified t) 9 10 (face font-lock-variable-name-face fontified t)) . 47) (undo-tree-id1829 . -1) (undo-tree-id1830 . -1) (undo-tree-id1831 . -1) (undo-tree-id1832 . -1) (undo-tree-id1833 . -1) (undo-tree-id1834 . -10) (undo-tree-id1835 . -10)) ((47 . 57) (3890 . 3897) ("WIMP" . 3890) (4010 . 4043) (#("Web-bASE" 0 5 (fontified nil) 5 6 (fontified nil) 6 7 (fontified nil) 7 8 (fontified nil)) . 4010) (undo-tree-id1735 . -5) (undo-tree-id1736 . -5) (undo-tree-id1737 . -5) (undo-tree-id1738 . -5) (undo-tree-id1739 . -5) (undo-tree-id1740 . -5) (undo-tree-id1741 . -5) (undo-tree-id1742 . -5) (undo-tree-id1743 . -5) (undo-tree-id1744 . -5) (undo-tree-id1745 . -5) (undo-tree-id1746 . -5) (undo-tree-id1747 . -5) (undo-tree-id1748 . -5) (undo-tree-id1749 . -5) (undo-tree-id1750 . -5) (undo-tree-id1751 . -5) (undo-tree-id1752 . -5) (4015 . 4018) (#("ased Image Mo" 0 12 (fontified nil) 12 13 (fontified nil)) . 4015) (undo-tree-id1753 . -12) (undo-tree-id1754 . -12) (undo-tree-id1755 . -12) (undo-tree-id1756 . -12) (undo-tree-id1757 . -12) (undo-tree-id1758 . -12) (undo-tree-id1759 . -12) 